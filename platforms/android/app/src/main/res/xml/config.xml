<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.VDAPP" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>VDAPP</name>
    <description>
	This is a Visitors Details App 
    </description>
    <author email="sebastianafadzie2@gmail.com" href="">
        Media Edge
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" height="57" platform="android" src="www/img/app.png" width="57" />
    <preference name="loglevel" value="DEBUG" />
</widget>
